{
  "name": "LuBan",
  "description" : "LuBan extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "luban.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens luban.html"
    }
  },
  "permissions": ["http://*.shanyishanmei.com/", "activeTab"]
}
